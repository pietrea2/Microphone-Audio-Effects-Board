--synthesis_resources = lut 16 
SUBDESIGN cmpr_92g
( 
	ageb	:	output;
	datab[12..0]	:	input;
) 
VARIABLE
	aeb_int	:	WIRE;
	agb_int	:	WIRE;
	dataa[12..0]	:	WIRE;
BEGIN 
	dataa[] = B"0000000000000";
	IF (dataa[] == datab[]) THEN
		aeb_int = VCC;
	ELSE
		aeb_int = GND;
	END IF;
	IF (dataa[] > datab[]) THEN
		agb_int = VCC;
	ELSE
		agb_int = GND;
	END IF;
	ageb = agb_int # aeb_int;
END;
--VALID FILE
